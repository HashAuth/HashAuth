{
    "$schema": "https://turborepo.org/schema.json",
    "tasks": {
        "build": {
            "dependsOn": ["^build"],
            "outputs": ["dist/**", "build/**"]
        },
        "test": {
            "dependsOn": [],
            "outputs": []
        },
        "clean": {
            "cache": false
        },
        "typecheck": {
            "outputs": []
        },
        "format": {
            "outputs": []
        },
        "lint": {
            "outputs": []
        }
    }
}
